<div class="main user-show">
  <div class="container">
    <div class="user">
      <img src="<%= "/users_images/#{@user.image_name}" %>">
      <h2><%=@user.name %></h2>
      <p><%=@user.email %></p>
      <%if @user.id==@current_user.id%>
        <%= link_to("Editing", "/users/#{@user.id}/edit") %>
      <%end%>
    </div>
  </div>
</div>
